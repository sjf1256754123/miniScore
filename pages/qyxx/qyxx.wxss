.gq_main{
  width: 98%;
  height: 100%;
  margin: 0 1%;
  padding-bottom:100rpx;
}
.header_title1{
  text-align: center;
  font-size: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #666;
}
.title{
text-align: center;
  font-size: 26rpx;
  padding-top: 20rpx;
  line-height: 40rpx;
  border-bottom: 1rpx solid #666;
}
.zscq11{
  display: flex;
}
.zscq11 .leftqy{
  width: 30%;
  height: 150rpx;
  line-height: 150rpx;
  font-size: 28rpx;
  text-align: center;
  border-bottom: 1rpx dotted #999;
}
.zscq11 .rightqy{
  width: 70%;
  height: 150rpx;
  line-height: 150rpx;
  font-size: 28rpx;
  text-align: center;
  border-bottom: 1rpx dotted #999;
}
.zscq11 view input{
  width: 80%;
  height: 60rpx;
  text-align: left;
  padding-left: 12rpx;
  border: solid 1rpx #999;
  border-radius: 12rpx;
  display: inline-table;
  vertical-align: middle;
}
.btn{
  clear: both;
  display: flex;
  flex-direction: row;
}

.btn .header_btn{
  flex: 1;
  margin: 20rpx;

}