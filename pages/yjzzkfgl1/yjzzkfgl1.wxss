}
.btn{
  clear: both;
  display: flex;
  flex-direction: row;
}

.btn .header_btn{
  flex: 1;
  margin: 20rpx;

}
.list-msg2 {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border: 1px solid #ccc;
  font-size: 24rpx;
  border-radius: 10rpx;
  padding: 0 10rpx;
  margin: 20rpx auto;
  overflow: hidden;
}
.list_name{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 22rpx;
  overflow: hidden;
}

.select_box {
  background-color: #999;
