.gq_main{
  width: 90%;
  height: 100%;
  margin: 0 5%;
  padding-bottom:100rpx;
}
.header_title1{
  text-align: center;
  font-size: 30rpx;
  padding-top: 20rpx;
}
.title{
text-align: center;
  font-size: 26rpx;
  padding-top: 20rpx;
  line-height: 40rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #666;
}
.jsxjx{
